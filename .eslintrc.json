{
    "root": true,
    "env": {
        "node": true
    },
    "extends": [
        "standard-with-typescript"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "brace-style": [2, "1tbs"],
        "comma-style": [2, "last"],
        "default-case": 2,
        "func-style": [2, "declaration"]
    }
}
